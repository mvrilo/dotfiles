[color]
  ui = true

[core]
  editor = /usr/bin/vim

[alias]
  ci = commit
  co = checkout
  stat = status
  st = status
  ll = log --pretty=oneline --abbrev-commit --max-count=30
  lg = log --pretty=oneline --abbrev-commit --graph --decorate --date=relative
  punch = !git pull && git push
